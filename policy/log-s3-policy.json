{
  "Version": "2012-10-17",
  "Id": "S3-Console-Auto-Gen-Policy-1736953029850",
  "Statement": [
    {
      "Sid": "S3PolicyStmt-DO-NOT-MODIFY-1736953028711",
      "Effect": "Allow",
      "Principal": {
        "Service": "logging.s3.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::wsi-logs-wlst/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "362708816803"
        }
      }
    },
    {
      "Sid": "AWSLogDeliveryWrite28229776",
      "Effect": "Allow",
      "Principal": {
        "Service": "delivery.logs.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::wsi-logs-wlst/cf-log/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "362708816803",
          "s3:x-amz-acl": "bucket-owner-full-control"
        },
        "ArnLike": {
          "aws:SourceArn": "arn:aws:logs:us-east-1:362708816803:delivery-source:CreatedByCloudFront-ELFYKNCAP7ETW"
        }
      }
    },
    {
      "Sid": "AWSCloudTrailAclCheck20150319-299c757c-c6c4-4230-bc43-b257b082a304",
      "Effect": "Allow",
      "Principal": {
        "Service": "cloudtrail.amazonaws.com"
      },
      "Action": "s3:GetBucketAcl",
      "Resource": "arn:aws:s3:::wsi-logs-wlst",
      "Condition": {
        "StringEquals": {
          "AWS:SourceArn": "arn:aws:cloudtrail:ap-northeast-2:362708816803:trail/wsi-cloudtrail"
        }
      }
    },
    {
      "Sid": "AWSCloudTrailWrite20150319-7d6e67a4-06d6-483f-8ec1-7042e3e8e442",
      "Effect": "Allow",
      "Principal": {
        "Service": "cloudtrail.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::wsi-logs-wlst/cloudtrail/AWSLogs/362708816803/*",
      "Condition": {
        "StringEquals": {
          "AWS:SourceArn": "arn:aws:cloudtrail:ap-northeast-2:362708816803:trail/wsi-cloudtrail",
          "s3:x-amz-acl": "bucket-owner-full-control"
        }
      }
    }
  ]
}
